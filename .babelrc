{
  "plugins": [
    [
      "inline-import", {
        "extensions": [".css"]
      }
    ]
  ],

  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            [
              "styled-jsx-plugin-sass",
              {
                "sassOptions": {
                  "includePaths": [
                    "styles"
                  ],
                  "precision": 2
                }
              }
            ]
          ]
        }
      }
    ]
  ],

  "env": {
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  }
}
